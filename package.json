    "dotenv": "^16.0.3",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.7.2",
    "hardhat": "^2.9.1",
    "hardhat-deploy": "^0.11.18",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "keccak256": "^1.0.6",
